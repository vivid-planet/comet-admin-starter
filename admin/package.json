    "scripts": {
        "postinstall": "npm run intl:update",
        "prestart": "npm run intl:compile",
        "start": "cross-env BABEL_ENV=development webpack-dev-server --config ./webpack.config.ts --env.production=0 -r dotenv/config",
        "prebuild": "npm run intl:compile",
        "build": "cross-env BABEL_ENV=production webpack --config ./webpack.config.ts --env.production=1 --progress",
        "tsc": "tsc --project ./tsconfig.json && :",
        "types": "tsc",
        "prelint": "npm run intl:compile",
        "lint": "npm run lint:eslint && npm run lint:tsc",
        "lint:eslint": "eslint --config ./.eslintrc.cli.js --ext .ts,.tsx,.js,.jsx,.json,.md src/",
        "lint:tsc": "tsc --project .",
        "intl:extract": "formatjs extract \"src/**/*.ts*\" --ignore ./**.d.ts --out-file lang-extracted/en.json --format simple",
        "intl:compile": "formatjs compile-folder --format simple --ast ./comet-admin-lang lang/comet-admin",
        "intl:update": "ts-node intl-update.ts"
    }
